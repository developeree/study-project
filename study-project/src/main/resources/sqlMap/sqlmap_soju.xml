<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sojuSql">
	
	<select id="selectSojuList" resultType="com.study_project.web.soju.model.Soju" parameterType="com.study_project.web.soju.model.Soju">
		SELECT 
			idx,
			title,
			price,
			company,
			tumbnail
		FROM
			soju
		WHERE 
			1 = 1
	</select>
	
</mapper>