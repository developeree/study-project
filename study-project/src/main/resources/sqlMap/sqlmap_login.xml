<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="loginSql">
	
	<select id="login" resultType="com.study_pjoject.web.model.User" parameterType="com.study_pjoject.web.model.User">
		SELECT
			*
		FROM
			user_info
		WHERE
			id = #{id}
		AND
			password = #{password}
	</select>
	
</mapper>